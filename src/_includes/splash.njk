---
layout: base.njk
---

{# space holder for animation #}

<div id="spacer"></div>
<div class="grid">
  <div class="grid-sizer"></div>
  <div class="grid-item grid-item--span biggy">
    <span class="title">Center for Concrete</span>
    <span class="title">+ Abstract Machines</span>
  </div>
  {% for n in collections.nav %}
  <div class="grid-item">
    <a href="{{ n.url }}">
      <div class="biggy">
        <h2>
          <span class="nav">{{ n.data.title }}</span>
        </h2>
      </div>
    </a>
  </div>
  {% endfor%}
</div>
