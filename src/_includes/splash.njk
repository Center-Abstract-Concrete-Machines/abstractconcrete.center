---
layout: base.njk
---

{# space holder for animation #}

<div id="spacer"></div>
<div class="biggy">
  <span class="title">Center for Concrete</span>
  <span class="title">+ Abstract Machines</span>
</div>
<div class="row masonry" data-masonry='{"percentPosition": true }'>
  {% for n in collections.nav %}
  <div class="col-12 item">
    <a href="{{ n.url }}">
      <div class="biggy">
        <h2>
          <span class="nav">{{ n.data.title }}</span>
        </h2>
      </div>
    </a>
  </div>
  {% endfor%}
</div>

<script>
  $(function () {
    var $wrapper = $(".masonry").imagesLoaded(function () {
      $wrapper.masonry({
        itemSelector: ".item",
      });
    });
  });
</script>
<script src="/assets/js/sketch.js"></script>

<script src="/assets/js/particle.js"></script>
<script src="/assets/js/drawer.js"></script>
